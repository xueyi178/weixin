<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>WeixinPay</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>pcPay</servlet-name>
    <servlet-class>com.java.servlet.PcPayServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>pcPay</servlet-name>
    <url-pattern>/pcPay</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoadPayImageServlet</servlet-name>
    <servlet-class>com.java.servlet.LoadPayImageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoadPayImageServlet</servlet-name>
    <url-pattern>/loadPayImage</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>PayNotifyUrlServlet</servlet-name>
    <servlet-class>com.java.servlet.PayNotifyUrlServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PayNotifyUrlServlet</servlet-name>
    <url-pattern>/payNotifyUrlServlet</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>LoadPayStatusServlet</servlet-name>
    <servlet-class>com.java.servlet.LoadPayStatusServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoadPayStatusServlet</servlet-name>
    <url-pattern>/loadPayStatusServlet</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>WebPayServlet</servlet-name>
    <servlet-class>com.java.servlet.WebPayServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WebPayServlet</servlet-name>
    <url-pattern>/webPay</url-pattern>
  </servlet-mapping>
</web-app>